{
	"compilerOptions": {
	  "module": "esnext",
	  "target": "es2020",
	  "moduleResolution": "node",
	  "lib": ["es2020"],
	  "esModuleInterop": true,
	  "resolveJsonModule": true,
	  "outDir": "dist",
	  "rootDir": "src",
	  "allowSyntheticDefaultImports": true,
	  "forceConsistentCasingInFileNames": true,
	  "strict": true,
	  "skipLibCheck": true
	}
  }